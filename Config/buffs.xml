<potato>
	<append xpath="/buffs">
		<buff name="buffCorrosiveRound">
			<damage_type value="radiation"/>
			<stack_type value="replace"/>
			<duration value="30"/>
			<update_rate value="1"/>
			<effect_group>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="#@modfolder:Resources/Mods.unity3d?CorrosiveShot" local_offset="0,0.3,0" parent_transform="Hips"/>
				<triggered_effect trigger="onSelfBuffFinish" action="RemovePrefabFromEntity" target="self" parent_transform="Hips" prefab="#@modfolder:Resources/Mods.unity3d?CorrosiveShot"/>
				<triggered_effect trigger="onSelfDied" action="RemovePrefabFromEntity" target="self" parent_transform="Hips" prefab="#@modfolder:Resources/Mods.unity3d?CorrosiveShot"/>
				<passive_effect name="HealthChangeOT" operation="base_subtract" duration="5,15,30" value="5,10,15"/>
					
			</effect_group>
		</buff>
		<buff name="buffExplosiveRound">
			<damage_type value="disease"/>
			<stack_type value="replace"/>
			<duration value="1"/>
			<effect_group><!--
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="#@modfolder:Resources/Mods.unity3d?ExplosiveShot" local_offset="0,0.3,0" parent_transform="Hips"/>
				<triggered_effect trigger="onSelfBuffFinish" action="RemovePrefabFromEntity" target="self" parent_transform="Hips" prefab="#@modfolder:Resources/Mods.unity3d?ExplosiveShot"/>
				<triggered_effect trigger="onSelfDied" action="RemovePrefabFromEntity" target="self" parent_transform="Hips" prefab="#@modfolder:Resources/Mods.unity3d?ExplosiveShot"/>-->
				<triggered_effect trigger="onSelfBuffStart" action="AddBuff" target="selfAOE" range="1" buff="buffExplosiveRoundAOE"/>
				<passive_effect name="HealthChangeOT" operation="base_subtract" value="85"/>
			</effect_group>
		</buff>
		<buff name="buffExplosiveRoundAOE">
			<damage_type value="disease"/>
			<stack_type value="replace"/>
			<duration value="1"/>
			<effect_group>
				<passive_effect name="HealthChangeOT" operation="base_subtract" value="40"/>
			</effect_group>
		</buff>
		<buff name="buffIncendiaryRound" >
			<damage_type value="disease"/>
			<stack_type value="replace"/>
			<duration value="15"/>
			<effect_group>
			<requirement name="EntityTagCompare" target="self" tags="zombie"/>
				<passive_effect name="HealthChangeOT" operation="base_subtract" duration="1,5,15" value="35,10,20"/>
				
				
				<triggered_effect trigger="onSelfBuffStart" action="AddBuff" target="self" buff="buffBurning"/>
				<!--<triggered_effect trigger="onSelfBuffStart" action="AddBuff" target="selfAOE" range="1" buff="buffIncendiaryRound"/>	-->
				<triggered_effect trigger="onSelfBuffUpdate" action="AddBuff" target="other" range="1" buff="buffBurning">
					<requirement name="RandomRoll" seed_type="Random" target="self" min_max="0,100" operation="LTE" value="80"/>
				</triggered_effect>
					
				
			</effect_group>
		</buff>
		<buff name="buffFlameThrower" >
			<damage_type value="disease"/>
			<stack_type value="replace"/>
			<duration value="8"/>
			<update_rate value="1"/>
			<effect_group>
			<requirement name="EntityTagCompare" target="self" tags="zombie"/>
				<passive_effect name="HealthChangeOT" operation="base_subtract" duration="1,3,8" value="45,10,20"/>
				
				
				<triggered_effect trigger="onSelfBuffStart" action="AddBuff" target="self" buff="buffBurning"/>
				<triggered_effect trigger="onSelfBuffStart" action="AddBuff" target="selfAOE" range="1" buff="buffFlameThrower">
					<requirement name="RandomRoll" seed_type="Random" target="self" min_max="0,100" operation="LTE" value="60"/>
				</triggered_effect>
					
				
			</effect_group>
		</buff>
		<buff name="buffLaser">
			<damage_type value="disease"/>
			<stack_type value="replace"/>
			<duration value="2"/>
			<update_rate value=".5"/>
			<effect_group>
				<passive_effect name="HealthChangeOT" operation="base_subtract" duration=".2,2" value="0,5000" />
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="#@modfolder:Resources/Mods.unity3d?LaserImpact" local_offset="0,0.4,0" parent_transform="Hips"/>
				<triggered_effect trigger="onSelfBuffFinish" action="RemovePrefabFromEntity" target="self" parent_transform="Hips" prefab="#@modfolder:Resources/Mods.unity3d?LaserImpact"/>
				<triggered_effect trigger="onSelfDied" action="RemovePrefabFromEntity" target="self" parent_transform="Hips" prefab="#@modfolder:Resources/Mods.unity3d?LaserImpact"/>
			</effect_group>
		</buff>
		<buff name="buffOrb">
			<damage_type value="disease"/>
			<stack_type value="replace"/>
			<duration value="10"/>
			<update_rate value="1"/>
			<effect_group>
			<passive_effect name="RunSpeed" operation="perc_set" value="0,.01,.1" duration="0,8,10"/>
				<passive_effect name="WalkSpeed" operation="perc_set" value="0,.01,.1" duration="0,8,10"/>
				<passive_effect name="CrouchSpeed" operation="perc_set" value="0" />
				<passive_effect name="JumpStrength" operation="perc_set" value="0" />
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="#@modfolder:Resources/Mods.unity3d?OrbPrefab" local_offset="0,0.2,0" parent_transform="Hips"/>
				<triggered_effect trigger="onSelfBuffFinish" action="RemovePrefabFromEntity" target="self" parent_transform="Hips" prefab="#@modfolder:Resources/Mods.unity3d?OrbPrefab"/>
				<triggered_effect trigger="onSelfDied" action="RemovePrefabFromEntity" target="self" parent_transform="Hips" prefab="#@modfolder:Resources/Mods.unity3d?OrbPrefab"/>
			</effect_group>
		</buff>
	</append>
</potato>
		